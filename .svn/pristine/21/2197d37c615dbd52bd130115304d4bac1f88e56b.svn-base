package com.hyva.hotel.pojo;

import java.sql.Date;

public class ExpensesPojo {
    private Long id;
    private String title;
    private Date date;
    private String attachment;
    private String remarks;
    private String expanses_category_id;
    private double amount;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public Date getDate() {
        return date;
    }

    public void setDate(Date date) {
        this.date = date;
    }

    public String getAttachment() {
        return attachment;
    }

    public void setAttachment(String attachment) {
        this.attachment = attachment;
    }

    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks;
    }

    public String getExpanses_category_id() {
        return expanses_category_id;
    }

    public void setExpanses_category_id(String expanses_category_id) {
        this.expanses_category_id = expanses_category_id;
    }

    public double getAmount() {
        return amount;
    }

    public void setAmount(double amount) {
        this.amount = amount;
    }
}
